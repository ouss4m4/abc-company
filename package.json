{
  "name": "angular-express-starter",
  "version": "0.0.0",
  "description": "angular express starter template",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "tsc": "npx tsc",
    "dev": "npx ts-node-dev ./src/server.ts",
    "prod": "npx tsc && node ./dist/server.js",
    "test": "jest --runInBand --forceExit --detectOpenHandles",
    "build:ng": "cd ngsource && npm i && ng build --prod --outputPath=../dist/static && cd ..",
    "build": "npx tsc ",
    "start":"node dist/index.js"
  },
  "keywords": [
    "express",
    "angular"
  ],
  "author": "Baaziz Oussama",
  "dependencies": {
    "express": "^4.17.2",
    "mongoose": "^6.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "license": "ISC"
}
